import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner se=new Scanner(System.in);
        int a=se.nextInt();
        int mul=1;
        int rem;
        while(a!=0){
            rem=a%10;
             mul=mul*rem;
             a=a/10;}
        System.out.println(mul);
    }
}
