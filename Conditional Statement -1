import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner se=new Scanner(System.in);
        int a=se.nextInt();
        
        if(a%3==0 && a%5==0){
             System.out.println("HiHello");}
        else if(a%3==0){
               System.out.println("Hi");}
        else{
            System.out.println("Hello");
        }
    }}
