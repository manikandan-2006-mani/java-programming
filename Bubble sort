import java.util.*;
public class Main{
    public static void main(String[]args){
        int arr[]={2,1,3,5,4,9,6,10,0};
        int i,j,temp;
        int len=arr.length;
        for( i=0;i<len;i++){
            for( j=i+1;j<len;j++){
                if(arr[i]>arr[j]){
                    temp=arr[i];
                    arr[i]=arr[j];
                    arr[j]=temp;
                }
            }
        }
        for(int k:arr){
            System.out.print(k+" ");
        }}
}
