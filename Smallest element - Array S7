import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner se=new Scanner(System.in);
        int s=se.nextInt();
        int arr[]=new int[s];
        for(int i=0;i<s;i++){
            arr[i]=se.nextInt();}
        Arrays.sort(arr);
        System.out.println(arr[0]);
    }
}
