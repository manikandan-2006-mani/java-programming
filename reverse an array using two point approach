import java.util.*;
public class Main{
    public static void main (String[]args){
        Scanner se=new Scanner(System.in);
        String name=se.next();
        char arr[]=name.toCharArray();
        int left=0,right=arr.length-1;
        char temp;
        while(left<=right){
            temp=arr[left];
            arr[left]=arr[right];
            arr[right]=temp;
            left++;
            right--;
        }
        System.out.println(new String(arr));
        
    }
}
