import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int original = num;
        int sumOfDigits = 0;

        while (num > 0) {
            sumOfDigits += num % 10;
            num /= 10;
        }

        if (sumOfDigits != 0 && original % sumOfDigits == 0) {
            System.out.println("Harshad Number");
        } else {
            System.out.println("Not Harshad Number");
        }
    }
}
