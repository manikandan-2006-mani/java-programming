import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner se=new Scanner(System.in);
        int rev=0;
        int rem;
        int a=se.nextInt();
        while(a!=0){
           rem=a%10;
         System.out.print(rem+" ");
         a=a/10;}
        
 }
}
